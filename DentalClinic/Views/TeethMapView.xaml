            <CheckBox Content="20"></CheckBox>
            <CheckBox Content="21"></CheckBox>
            <CheckBox Content="22"></CheckBox>
            <CheckBox Content="23"></CheckBox>
            <CheckBox Content="24"></CheckBox>
            <CheckBox Content="25"></CheckBox>
            <CheckBox Content="26"></CheckBox>
            <CheckBox Content="27"></CheckBox>
            <CheckBox Content="28"></CheckBox>
            <CheckBox Content="29"></CheckBox>
            <CheckBox Content="30"></CheckBox>
            <CheckBox Content="31"></CheckBox>
            <CheckBox Content="32"></CheckBox>


        </ListBox>
        <Button Grid.Row="1"
                
                Margin="5"
                Name="ButtonShowResults"
                Content="Save"
                Click="ButtonShowResults_Click"
                ></Button>
    </Grid>

</Window>
