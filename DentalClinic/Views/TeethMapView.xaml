            <CheckBox Content="12"></CheckBox>
            <CheckBox Content="13"></CheckBox>
            <CheckBox Content="14"></CheckBox>
            <CheckBox Content="15"></CheckBox>
            <CheckBox Content="16"></CheckBox>
            <CheckBox Content="17"></CheckBox>
            <CheckBox Content="18"></CheckBox>
